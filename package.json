{
  "name": "gatsby-starter-hello-world",
  "private": true,
  "description": "A simplified bare-bones starter for Gatsby",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "patch-package",
    "prebuild": "postcss src/styles/ --dir public/styles/",
    "build": "toast incremental",
    "postbuild": "ps aux | grep postcss | awk '{print $2}' | xargs kill -9 > /dev/null 2>&1 && exit 1",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "breadbox": "breadbox --dest public/web_modules"
  },
  "dependencies": {
    "@jlengstorf/react-tweet-embed": "^1.0.1",
    "@sector/breadbox": "0.0.4",
    "babel-plugin-inline-react-svg": "^1.1.1",
    "babel-plugin-transform-postcss": "^0.3.0",
    "gatsby-image": "^2.4.12",
    "gatsby-transformer-cloudinary": "^0.3.5",
    "globby": "^11.0.1",
    "matter-js": "^0.14.2",
    "node-fetch": "^2.6.0",
    "patch-package": "^6.2.2",
    "postcss": "^7.0.32",
    "postcss-cli": "^7.1.1",
    "postcss-modules": "^3.2.1",
    "preact": "^10.4.5",
    "react-helmet": "^6.1.0",
    "toast": "^0.2.9",
    "use-sound": "^1.0.2"
  },
  "devDependencies": {
    "prettier": "2.0.5"
  },
  "snowpack": {
    "installOptions": {
      "alias": {
        "react": "preact/compat"
      },
      "knownEntryPoints": [
        "preact"
      ]
    }
  }
}
